{
  "name": "crypto-volatility",
  "version": "1.0.0",
  "description": "Modern TypeScript library to calculate volatility and DVOL for BTC and SOL using CoinGecko API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "example": "tsx examples/usage.ts",
    "example:jupiter": "tsx examples/jupiter.ts",
    "example:coinlore": "tsx examples/coinlore.ts",
    "example:coindesk": "tsx examples/coindesk.ts",
    "example:coindesk-sdk": "tsx examples/coindesk-sdk.ts",
    "example:simple": "tsx examples/simple.ts",
    "example:quickstart": "tsx examples/quickstart.ts",
    "example:advanced": "tsx examples/advanced.ts",
    "example:typescript": "tsx examples/typescript.ts",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "undici": "^7.16.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.0",
    "typescript": "^5.3.0",
    "tsx": "^4.0.0"
  },
  "keywords": [
    "crypto",
    "volatility",
    "bitcoin",
    "solana",
    "dvol",
    "coingecko",
    "typescript"
  ]
}
